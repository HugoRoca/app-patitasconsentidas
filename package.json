{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "copyfiles": "cpy src/utils/*.yaml dist/utils/",
    "clean": "rimraf .cache dist reports tmp",
    "lint:code": "eslint --ext ts src/ -f stylish",
    "lint:test": "eslint --ext ts test/ -f stylish",
    "lint:report": "eslint --ext ts src/ -f html -o reports/lint.html",
    "format": "prettier --write src/**/*.ts",
    "format:fix": "eslint --ext ts src --fix",
    "predevelop": "npm run clean && npm run lint:code && npm run copyfiles",
    "develop": "nodemon ./src/server.ts",
    "prebuild": "npm run clean && npm run lint:report",
    "prestart": "npm run clean && npm run lint:report && npm run build && npm run copyfiles",
    "start": "cross-env NODE_ENV=stage node ./dist/server.js",
    "start:production": "cross-env NODE_ENV=production node ./dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.20.0",
    "blueimp-md5": "^2.16.0",
    "cross-env": "^7.0.2",
    "fancy-log": "^1.3.3",
    "http-errors": "^1.7.3",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^3.1.0",
    "koa-helmet": "^5.2.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.11",
    "typescript": "^3.9.5",
    "yenv": "^2.1.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/blueimp-md5": "^2.7.0",
    "@types/fancy-log": "^1.3.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-router": "^7.4.0",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.0.6",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "tsc-watch": "^4.2.8"
  }
